{
  "manifest_version": 2,

  "name": "Redirector",
  "description": "This extension redirect to specific page according to url data.",
  "version": "1.0",

  "background": {"scripts":["redirect.js"]},
  "options_page": "options.html",

  "permissions": [
    "webRequest",
	"*://*/*",
	"webRequestBlocking",
	"storage"
  ]

}
